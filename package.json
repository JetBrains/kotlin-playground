    "@babel/runtime-corejs2": "~7.4.4",
    "babel-loader": "v8.0.6",
    "babel-plugin-async-to-promises": "^1.0.5",
    "ci-publish": "^1.3.1",
    "codemirror": "^5.48.4",
    "codemirror-grammar-mode": "^0.1.9",
    "css-loader": "2.1.1",
    "debounce": "^1.2.0",
